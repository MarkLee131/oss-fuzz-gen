    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Report_Generation",
            "type": "debugpy",
            "request": "launch",
            "program": "./report/web.py",
            "console": "integratedTerminal",
            "args": [
                "-r",
                "./results",
                "-o",
                "./kx_spec"
            ]
        },
        {
            "name": "Python Debugger: Spec_Driver_Generation",
            "type": "debugpy",
            "request": "launch",
            "program": "./run_all_experiments.py",
            "console": "integratedTerminal",
            "args": [
                "--model",
                "gpt-4o",
                "-y",
                "./benchmark-sets/comparison/avahi.yaml",
                "--context",
                "--template-directory=prompts/template_spec",
                "mf"
            ]
        }
    ]
}